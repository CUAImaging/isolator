{
    "name": "icecave/isolator",
    "description": "Dependency injection for global functions.",
    "keywords": [
        "test",
        "phpunit",
        "unit",
        "mock",
        "fake",
        "double",
        "stub"
    ],
    "homepage": "https://github.com/icecave/isolator",
    "license": "MIT",
    "authors": [
        {
            "name": "James Harris",
            "email": "mailjamesharris@gmail.com",
            "homepage": "https://github.com/jmalloc"
        },
        {
            "name": "Erin Millard",
            "email": "ezzatron@gmail.com",
            "homepage": "http://ezzatron.com/"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2",
        "phake/phake": "^1",
        "phpunit/phpunit": "^7"
    },
    "autoload": {
        "psr-4": {
            "Icecave\\Isolator\\": "src"
        },
        "files": [
            "src/register-autoloader.php"
        ]
    },
    "autoload-dev": { "psr-4": { "Icecave\\Isolator\\": "test/src" } },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "7.1"
        }
    }
}
